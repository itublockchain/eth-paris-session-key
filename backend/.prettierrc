{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 4,
  "printWidth": 80,
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "decorators-legacy",
    "jsx"
  ]
}